includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app
        - bootstrap/app.php
        - config
        - database
    level: 6
    tmpDir: storage/framework/phpstan
    parallel:
        maximumNumberOfProcesses: 4
    ignoreErrors:
        - '#Offset .+ does not exist on array#'
    bootstrapFiles:
        - bootstrap/app.php
